<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.gtt.srs.entity.AreaProvince" table="area_province" schema="srs">
        <id name="provinceId">
            <column name="province_id" sql-type="int(11)"/>
        </id>
        <property name="provinceName">
            <column name="province_name" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>