<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.gtt.srs.entity.RecordHistory" table="record_history" schema="srs">
        <id name="recordId">
            <column name="record_id" sql-type="int(11)"/>
        </id>
        <property name="misspersonId">
            <column name="missperson_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>