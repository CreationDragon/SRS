<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.gtt.srs.entity.Personspic" table="personspic" schema="srs">
        <id name="picId">
            <column name="pic_id" sql-type="int(11)"/>
        </id>
        <property name="personsId">
            <column name="persons_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="picName">
            <column name="pic_name" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>