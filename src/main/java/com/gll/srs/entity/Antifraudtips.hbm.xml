<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.gll.srs.entity.Antifraudtips" table="antifraudtips" schema="srs">
        <id name="tipsId">
            <column name="tips_id" sql-type="int(11)"/>
        </id>
        <property name="tipsTitle">
            <column name="tips_title" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="tipsContent">
            <column name="tips_content" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="tipsDate">
            <column name="tips_date" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>