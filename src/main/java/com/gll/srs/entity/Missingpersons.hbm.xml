<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.gll.srs.entity.Missingpersons" table="missingpersons" schema="srs">
        <id name="personsId">
            <column name="persons_id" sql-type="int(11)"/>
        </id>
        <property name="personsName">
            <column name="persons_name" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="personsAge">
            <column name="persons_age" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="personsBodyheight">
            <column name="persons_bodyheight" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="personsFeature">
            <column name="persons_feature" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
        <property name="personsAddress">
            <column name="persons_address" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
        <property name="personsDna">
            <column name="persons_DNA" sql-type="int(1)" not-null="true"/>
        </property>
        <property name="personsDateDiscovered">
            <column name="persons_DateDiscovered" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="personsDiscoverySites">
            <column name="persons_DiscoverySites" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
        <property name="personsContact">
            <column name="persons_Contact" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="personsRescueunit">
            <column name="persons_rescueunit" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
        <property name="personsReleasedate">
            <column name="persons_RELEASEDATE" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="personsNote">
            <column name="persons_Note" sql-type="varchar(300)" length="300" not-null="true"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="missState">
            <column name="miss_state" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="personsGender">
            <column name="persons_gender" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="personsDress">
            <column name="persons_dress" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="psersonsPic">
            <column name="psersons_pic" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int(1)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>